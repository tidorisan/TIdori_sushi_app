<h1>Users::Coupons#index</h1>
<p>Find me in app/views/users/coupons/index.html.erb</p>

<td><%= link_to "クーポン登録", new_users_store_coupon_path(@store_id) %></td>

	<table>
		<caption>クーポン一覧画面</caption>
		<thead>
			<tr>
				<th>対象</th>
				<th>表示</th>
			</tr>
		</thead>
		<tbody>
			<% @coupons.each do |coupon| %>
				<tr>
					<td><%= coupon.target %></td>
					<% if coupon.enabled_status == true %>
						<td><p>有効</p></td>
					<% else %>
						<td><p>無効</p></td>
					<% end %>
					<td><%= link_to "編集画面へ", edit_users_store_coupon_path(coupon.id, coupon.store_id) %></td>
				</tr>
			<% end %>
		</tbody>
	</table>